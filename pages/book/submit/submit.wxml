<form bindsubmit="booksubmit" reportSubmit="true"><view class="population"><view class="goods flex-row"><view class="goods-img flex-grow-0"><image src="{{goods.cover_pic}}"></image></view><view class="flex-grow-1 flex-col goods-info"><view class="flex-grow-1 goods-name">\n                    {{goods.name}}\n</view><view class="flex-gorw-0 flex-row money flex-y-bottom"><view class="original_price flex-grow-0">\n                        ¥{{goods.original_price}}\n</view><view class="price flex-grow-0">\n                        ¥{{goods.price}}\n</view></view></view></view><view class="form"><view class="form-image"><image src="{{__wxapp_img.yy.form_title.url}}"></image></view><view class="group-form"><block wx:for="{{form_list}}" wx:for-item="item" wx:for-index="index"><block wx:if="{{item.type=='text'}}" ><view class="username flex-row input-shell flex-y-center"><view class="flex-grow-0">\n                                {{item.name}}：\n</view><view class="flex-grow-1"><input bindinput="inputChenge" data-id="{{index}}" placeholder="{{item.tip}}" value="{{item.default}}"></input></view></view></block><block wx:if="{{item.type=='radio'}}" ><view class="group-radio flex-row input-shell flex-y-center"><view class="flex-grow-0">\n                                {{item.name}}：\n</view><view class="flex-grow-1" style="padding-top:24rpx;"><block wx:for="{{item.default}}" wx:for-item="de" wx:for-index="idx"><label bindtap="radioChange" class="checkbox group-radio-item {{de.selected?'active':''}} flex-y-center" data-id="{{idx}}" data-pid="{{index}}"><checkbox checked="{{item.selected}}" data-id="0" style="display:none" value="{{de.name}}"></checkbox>{{de.name}}\n</label></block></view></view></block><block wx:if="{{item.type=='checkbox'}}" ><view class="group-radio flex-row input-shell flex-y-center"><view class="flex-grow-0">\n                                {{item.name}}：\n</view><view class="flex-grow-1" style="padding-top:24rpx;"><block wx:for="{{item.default}}" wx:for-item="de" wx:for-index="idx"><label bindtap="checkboxChange" class="checkbox group-radio-item {{de.selected?'active':''}} flex-y-center" data-id="{{idx}}" data-pid="{{index}}"><checkbox checked="{{item.selected}}" data-id="0" style="display:none" value="{{de.name}}"></checkbox>{{de.name}}\n</label></block></view></view></block><block wx:if="{{item.type=='time'}}" ><view class="username flex-row input-shell flex-y-center"><view class="flex-grow-0">\n                                {{item.name}}：\n</view><view class="flex-grow-1 flex-row picker-data flex-y-center"><picker bindchange="inputChenge" class="flex-grow-0" data-id="{{index}}" mode="time" start="{{now_date}}" style="padding-right:20rpx;" value="{{item.default}}"><view class="picker flex-y-center">\n                                        {{item.default}}\n</view></picker><view class="flex-grow-0 picker-time flex-y-center"><image src="{{__wxapp_img.share.right.url}}"></image></view></view></view></block><block wx:if="{{item.type=='date'}}" ><view class="username flex-row input-shell flex-y-center"><view class="flex-grow-0">\n                                {{item.name}}：\n</view><view class="flex-grow-1 flex-row picker-data flex-y-center"><picker bindchange="inputChenge" class="flex-grow-0" data-id="{{index}}" mode="date" start="{{now_date}}" style="padding-right:20rpx;" value="{{item.default}}"><view class="picker flex-y-center">\n                                        {{item.default}}\n</view></picker><view class="flex-grow-0 picker-time flex-y-center"><image src="{{__wxapp_img.share.right.url}}"></image></view></view></view></block><block wx:if="{{item.type=='textarea'}}" ><view class="username flex-row input-shell flex-y-top" style="padding:24rpx 0;"><view class="flex-grow-0 flex-y-top">\n                                {{item.name}}：\n</view><view class="flex-grow-1 flex-row flex-y-center"><textarea bindinput="inputChenge" data-id="{{index}}" placeholder="{{item.tip}}"></textarea></view></view></block><block wx:if="{{item.type=='uploadImg'}}" ><view class="username flex-row input-shell flex-y-center" style="padding:20rpx 0;"><view class="flex-grow-0 flex-y-center">{{item.name}}：</view><view bindtap="uploadImg" class="flex-grow-1 flex-y-center" data-id="{{index}}"><input data-id="100" style="display:none" value="{{item.default}}"></input><block wx:if="{{item.default}}" ><view style="text-align: right;width: 100%;font-size: 0"><image mode="aspectFit" src="{{item.default}}" style="width:88rpx;"></image></view></block><block wx:else ><view style="text-align:right;color:#c9c9c9;width:100%">{{item.tip?item.tip:'请选择图片'}}</view></block></view><view class="flex-grow-0 flex-y-center"><image src="{{__wxapp_img.balance.right.url}}" style="width:12rpx;height:18rpx;margin-left:18rpx;"></image></view></view></block></block><view class="group-radio flex-row input-shell flex-y-center"><view class="flex-grow-0">\n                        支付方式：\n</view><view class="flex-grow-1" style="padding-top:24rpx;"><block wx:if="{{wechat}}" ><label bindtap="switch" class="checkbox group-radio-item {{pay_type=='WECHAT_PAY'?'active':''}} flex-y-cente" data-type="WECHAT_PAY">\n                            微信支付\n</label></block><block wx:if="{{balance}}" ><label bindtap="switch" class="checkbox group-radio-item {{pay_type=='BALANCE_PAY'?'active':''}} flex-y-cente" data-type="BALANCE_PAY">\n                            余额支付\n</label></block></view></view></view></view></view><button class="submit-btn flex-row flex-y-center flex-x-center" formType="submit">\n        提交预约\n</button></form>
