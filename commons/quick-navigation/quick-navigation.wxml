<block wx:if="{{quick_icon}}" ><view class="quick-icon" direction="all"><view bindtap="quickNavigation" class="flex-col flex-y-center flex-x-center quick-bg"><text class="quick-text">快捷</text><text class="quick-text">导航</text></view></view></block><block wx:else ><view class="act-modal"><view bindtap="closeActModal" class="act-modal-bg"></view><view class="act-modal-pic flex-y-center flex-x-center"><view class="quick-icon" direction="all"><view style="position:relative;"><navigator hoverClass="none" openType="reLaunch" url="/pages/index/index"><image animation="{{animationPlus}}" class="quick-dial" mode="aspectFit" src="/images/quick-home.png" style="opacity:0;position:absolute;"></image></navigator><block wx:if="{{store.dial==1&&(store.dial_pic)}}" ><view animation="{{animationPic}}" bindtap="to_dial" class="quick-dial" style="background-image:url('{{store.dial_pic}}'); background-size: 100rpx 100rpx;"></view></block><block wx:if="{{store.option.web_service}}" ><navigator hoverClass="none" openType="navigate" url="/pages/web/web?url={{store.option.web_service_url}}"><image animation="{{animationcollect}}" class="quick-dial" src="{{store.option.web_service}}" style="opacity:0;position:absolute;"></image></navigator></block><block wx:if="{{store.show_customer_service&&(store.show_customer_service==1)&&(store.service)}}" ><button openType="contact" sessionFrom="{{__user_info.nickname}}|" style="padding:0px;display:inline;background-color:transparent;"><image animation="{{animationTranspond}}" class="quick-dial" src="{{store.service}}" style="opacity:0;position:absolute;"></image></button></block><block wx:if="{{store.option.wxapp.pic_url}}" ><navigator appId="{{store.option.wxapp.appid}}" data-open_type="wxapp" hoverClass="none" openType="navigate" path="{{store.option.wxapp.path}}" target="miniProgram"><image animation="{{animationInput}}" class="quick-dial" src="{{store.option.wxapp.pic_url}}" style="opacity:0;position:absolute;"></image></navigator></block><view bindtap="quickNavigation" class="flex-col flex-y-center flex-x-center quick-bg" style="z-index:20;position:relative;bottom:0"><text class="quick-text">收起</text></view></view></view></view></view></block>
