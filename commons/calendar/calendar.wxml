<view class="top-handle"><view bindtap="doDay" class="prev" data-key="left"><image src="{{__wxapp_img.register.left.url}}"></image></view><view class="today">{{currentDate}}</view><view bindtap="doDay" class="next" data-key="right"><image src="{{__wxapp_img.register.right.url}}"></image></view></view><view class="week"><block wx:for="{{weeks}}" wx:for-item="item" wx:for-index="index"><view class="weeks-day">{{item.day}}</view></block></view><view class="box-flex"><block wx:for="{{currentDayList}}" wx:key="{{key}}" wx:for-item="vo" wx:for-index="key"><view class="flex-item"><block wx:if="{{vo.is_re==1}}" ><block wx:if="{{currentDay!=vo.date}}" ><view bindtap="selectDay" class="item-content {{!selectCSS}}" data-day="{{vo.date}}"><image class="re_pic" src="{{__wxapp_img.register.quan.url}}"></image>\n                {{vo.date}}\n</view></block></block><block wx:else ><block wx:if="{{currentDay!=vo.date}}" ><view bindtap="selectDay" class="item-content {{!selectCSS}}" data-day="{{vo.date}}">\n                {{vo.date}}\n</view></block></block><block wx:if="{{vo.is_re==1}}" ><block wx:if="{{currentDay==vo.date}}" ><view bindtap="selectDay" class="item-content {{selectCSS}}" data-day="{{vo.date}}"><image class="re_pic" src="{{__wxapp_img.register.quan.url}}"></image>\n                {{vo.date}}\n</view></block></block><block wx:else ><block wx:if="{{currentDay==vo.date}}" ><view bindtap="selectDay" class="item-content {{selectCSS}}" data-day="{{vo.date}}">\n                {{vo.date}}\n</view></block></block></view></block></view>
